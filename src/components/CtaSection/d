server {
    listen 80;
    server_name cidadesdf.datasavvy.com www.cidadesdf.datasavvy.com;

    location / {
        proxy_pass http://localhost:4000;
        proxy_http_version 1.1;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection 'upgrade';
        proxy_set_header Host $host;
        proxy_cache_bypass $http_upgrade;
    }
}

149.100.142.100

sudo ln -s /etc/nginx/sites-available/cidadesdf.datasavvy.com.br /etc/nginx/sites-enabled/


sudo certbot --nginx -d cidadesdf.datasavvy.com.br -d www.cidadesdf.datasavvy.com.br
